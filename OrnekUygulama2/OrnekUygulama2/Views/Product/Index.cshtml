<h3>View Bag</h3>

<ul>    
    @foreach (var product in ViewBag.products as List<OrnekUygulama2.Models.Product>)
    {
        <li>@product.ProductName</li>
    }
</ul>

<h3>View Data</h3>

<ul>
    @foreach (var product in ViewData["products"] as List<OrnekUygulama2.Models.Product>)
    {
        <li>@product.ProductName</li>
    }
</ul>

<h3>TempData</h3>
<ul>
    @foreach (var product in TempData["products"] as List<OrnekUygulama2.Models.Product>)
    {
        <li>@product.ProductName</li>
    }
</ul>